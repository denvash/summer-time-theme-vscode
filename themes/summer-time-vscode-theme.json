  "semanticHighlighting": true,
  "semanticTokenColors": {
    "function.declaration.readonly": {
      "foreground": "#ECF6FF",
      "fontStyle": "underline"
    },
    "property.declaration": {
      "foreground": "#ECF6FF",
      "fontStyle": "bold"
    },
    "function.readonly": {
      "foreground": "#ECF6FF",
      "fontStyle": "italic underline"
    },
    "parameter.declaration": {
      "foreground": "#ECF6FF",
      "fontStyle": ""
    },
    "function.readonly.local": {
      "foreground": "#ECF6FF",
      "fontStyle": "italic underline"
    },
    "variable.declaration.readonly": {
      "foreground": "#EBEA8B",
      "fontStyle": "italic"
    },
    "variable.readonly": {
      "foreground": "#EBEA8B",
      "fontStyle": ""
    },
    "*.readonly": {
      "foreground": "#EBEA8B",
      "fontStyle": ""
    },
    "property": {
      "foreground": "#AEE9F5",
      "fontStyle": ""
    },
    "parameter": {
      "foreground": "#AEE9F5",
      "fontStyle": ""
    },
    "type": {
      "foreground": "#A8F79A",
      "fontStyle": "italic"
    },
    "type.declaration": {
      "foreground": "#A8F79A",
      "fontStyle": "italic"
    },
    "interface.defaultLibrary": {
      "foreground": "#A8F79A",
      "fontStyle": "italic"
    },
    "variable.defaultLibrary": {
      "foreground": "#A8F79A",
      "fontStyle": "bold"
    },
    "member.defaultLibrary": {
      "foreground": "#AEE9F5",
      "fontStyle": "italic"
    },
    "*.defaultLibrary": {
      "foreground": "#AEE9F5",
      "fontStyle": "italic"
    }
  },
